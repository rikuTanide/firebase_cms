{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/",
        "function": "index"
      },
      {
        "source": "/tech_reviews/**",
        "function": "tech_reviews"
      },
      {
        "source": "/book_reviews/**",
        "function": "book_reviews"
      }
    ]
  }
}
